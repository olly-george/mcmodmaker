<Window x:Class="MinecraftModMaker.ConfirmActionDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MinecraftModMaker"
        mc:Ignorable="d"
        Title="" Height="235.909" Width="320" WindowStyle="ToolWindow" ShowInTaskbar="False">
    <Grid Margin="0,0,22,-1">
        <Label x:Name="messageLabel" Content="sd" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Width="280" Height="77" Background="{x:Null}" Foreground="Black"/>
        <Button x:Name="okButton" Content="Ok" HorizontalAlignment="Left" Margin="0,134,0,0" VerticalAlignment="Top" Width="95" Click="OkButton_Click" Height="39"/>
        <Button x:Name="cancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="120,134,0,0" VerticalAlignment="Top" Width="113" Click="CancelButton_Click" Height="39"/>
        <CheckBox x:Name="dontShowAgainCheckBox" Content="Don't show again" HorizontalAlignment="Left" Margin="27,97,0,0" VerticalAlignment="Top" Width="187"/>
    </Grid>
</Window>
